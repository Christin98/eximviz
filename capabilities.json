{
  "dataRoles": [
    {
      "displayName": "Column",
      "name": "column",
      "kind": "Grouping"
    },
    {
      "displayName": "Value",
      "name": "value",
      "kind": "Measure"
    }
  ],
  "objects": {
    "circle": {
      "displayName": "Circle",
      "properties": {
        "circleColor": {
          "displayName": "Color",
          "description": "The fill color of the circle.",
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        },
        "circleThickness": {
          "displayName": "Thickness",
          "description": "The circle thickness.",
          "type": {
            "numeric": true
          }
        }
      }
    },
    "grid": {
      "displayName": "Grid",
      "properties": {
        "gridKey": {
          "displayName": "Key",
          "description": "ag-Grid Enterprise License Key.",
          "type": {
            "text": true
          }
        },
        "gridFilter": {
          "displayName": "Filter",
          "description": "ag-Grid Column Header Filters.",
          "type": {
            "bool": true
          }
        }
      }
    }
  },
  "dataViewMappings": [
    {
      "conditions": [],
      "table": {
        "rows": {
          "select": [
            {
              "for": {
                "in": "column"
              }
            },
            {
              "for": {
                "in": "value"
              }
            }
          ],
          "dataReductionAlgorithm": {
            "window": {
              "count": 200000
            }
          }
        }
      }
    }
  ],
  "supportsLandingPage": true,
  "supportsEmptyDataView": true,
  "suppressDefaultTitle": true,
  "privileges": [ 
    {
      "name": "ExportContent",
      "essential": true
    },
    {
      "name": "WebAccess",
      "essential": true,
      "parameters": ["https://*.microsoft.com", "https://localhost:8080", "http://localhost:8080", "https://*.powerbi.com"]
    }
  ]
}
